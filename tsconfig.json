{
  "baseURL": "./",
  "include": ["src/**/*.js", "src/**/*.d.ts"],
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "declaration": true,
    "module": "es2020",
    "checkJs": true,
    "noImplicitAny": false,
    "strict": true,
    "strictBindCallApply": true,
    "downlevelIteration": true,
    "lib": ["ES2020"],

    "typeRoots": ["./vendor/moddables/typings/", "typings/"],
    "paths": {
      "*": ["./src/host/*", "./src/mod/*", "./src/lib/*", "./src/mods/*"],
      "Resource": ["./vendor/moddable/typings/Resource"],
      "base64": ["./vendor/moddable/typings/base64"],
