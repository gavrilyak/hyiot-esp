      "piu/Sound": ["./vendor/moddable/typings/piu/Sound"],
      "piu/Timeline": ["./vendor/moddable/typings/piu/Timeline"],
      "piu/WipeTransition": ["./vendor/moddable/typings/piu/WipeTransition"],
      "dns/parser": ["./vendor/moddable/typings/dns/parser"],
      "dns/serializer": ["./vendor/moddable/typings/dns/serializer"],
      "dns/server": ["./vendor/moddable/typings/dns/server"],
      "mc/config": ["./vendor/moddable/typings/mc/config"],
