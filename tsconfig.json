      "dns": ["./vendor/moddable/typings/dns"],
      "file": ["./vendor/moddable/typings/file"],
      "flash": ["./vendor/moddable/typings/flash"],
      "hex": ["./vendor/moddable/typings/hex"],
      "http": ["./vendor/moddable/typings/http"],
      "instrumentation": ["./vendor/moddable/typings/instrumentation"],
      "mdns": ["./vendor/moddable/typings/mdns"],
      "net": ["./vendor/moddable/typings/net"],
      "ping": ["./vendor/moddable/typings/ping"],
      "preference": ["./vendor/moddable/typings/preference"],
      "serial": ["./vendor/moddable/typings/serial"],
      "sntp": ["./vendor/moddable/typings/sntp"],
      "socket": ["./vendor/moddable/typings/socket"],
      "telnet": ["./vendor/moddable/typings/telnet"],
      "time": ["./vendor/moddable/typings/time"],
      "timer": ["./vendor/moddable/typings/timer"],
      "uuid": ["./vendor/moddable/typings/uuid"],
      "websocket": ["./vendor/moddable/typings/websocket"],
      "wifi": ["./vendor/moddable/typings/wifi"],
      "worker": ["./vendor/moddable/typings/worker"],
      "zip": ["./vendor/moddable/typings/zip"],
      "commodetto/Bitmap": ["./vendor/moddable/typings/commodetto/Bitmap"],
      "commodetto/Poco": ["./vendor/moddable/typings/commodetto/Poco"],
      "commodetto/parseBMF": ["./vendor/moddable/typings/commodetto/parseBMF"],
      "commodetto/parseBMP": ["./vendor/moddable/typings/commodetto/parseBMP"],
      "commodetto/parseRLE": ["./vendor/moddable/typings/commodetto/parseRLE"],
