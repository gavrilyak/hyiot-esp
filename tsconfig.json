{
  "baseURL": "./",
  "include": [
    "./src/**/*.js",
    "./src/**/*.d.ts",
    "./moddable/typings/**/*.d.ts",
    "./moddable/modules/**/*.js"
  ],
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2021",
    "downlevelIteration": true,
    "declaration": true,
    "module": "esnext",
    "checkJs": true,
    "allowJs": true,
    "noImplicitAny": false,
    "strict": true,
    "strictBindCallApply": true,
    "lib": ["ES2021"],
    "moduleResolution": "classic",
    "paths": {
      "embedded:io/digital": ["./moddable/modules/io/digital/digital"],
      "embedded:io/digitalbank": ["./moddable/modules/io/digital/digitalbank"],
      "*": [
        "./src/host/*",
        "./src/mod/*",
        "./src/lib/*",
        "./src/mods/*",
        "./moddable/typings/*"
      ]
    }
  }
}
